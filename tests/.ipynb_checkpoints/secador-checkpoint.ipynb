    "def CalcRecSecador(xfs,xfw,xps,xpw):\n",
    "    #base de cálculo\n",
    "    F=100\n",
    "    #bm sólido\n",
    "    P=100*xfs/xps\n",
    "    W=F*xfw-P*xpw\n",
    "    Rec=W/(F*xfw)*100\n",
    "    return P,W,Rec\n",
    "\n",
    "P, W, Rec = CalcRecSecador(.8,.2,.96,.04)\n",
    "\n",
    "print(\"P, W, Rec %\")\n",
    "print(P, W, Rec)"
   ]
  }
